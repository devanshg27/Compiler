int arr[1000];

int main() {
	int n;

	print("Enter number of elements: ");
	read(n);
	print("Enter arr elements: ");

	for (int i = 0; i < n; i=i+1) {
		read(arr[i]);
	}

	for (int i = 1 ; i <= n - 1; i=i+1) {
		int j = i;

		while ( j > 0 and arr[j-1] > arr[j]) {
			int temp = arr[j];
			arr[j] = arr[j-1];
			arr[j-1] = temp;
			j = j-1;
		}
	}

	for (int i = 0; i < n; i=i+1) {
		print(arr[i], " ");
	}

	return 0;
}