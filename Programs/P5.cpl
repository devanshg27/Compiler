bool seq[20];

void print_combinations(int pos, int n) {
	if(pos == n+1) {
		for(int i=1; i<=n; i=i+1) {
			if(seq[i]) {
				print(i, " ");
			}
		}
		print('\n');
		return;
	}

	seq[pos] = false;
	print_combinations(pos+1, n);
	seq[pos] = true;
	print_combinations(pos+1, n);
	return;
}

	

int main() {
	int n;

	print("Enter n: ");
	read(n);
	
	print_combinations(1, n);
}
